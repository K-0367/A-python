password = input("Enter your password: ")

length = 0
upper = 0
lower = 0
digit = 0
special = 0

for ch in password:
    length = length + 1
    
    if ch.isupper():
        upper = 1
    elif ch.islower():
        lower = 1
    elif ch.isdigit():
        digit = 1
    else:
        special = 1

if length >= 8 and upper == 1 and lower == 1 and digit == 1 and special == 1:
    print("Strong Password")

elif length >= 6 and (upper == 1 or lower == 1) and digit == 1:
    print("Medium Password")

else:
    print("Weak Password")
